};

&sde_dsi_active {
	mux {
		pins = "gpio75", "gpio76";
		function = "gpio";
	};

	config {
		pins = "gpio75", "gpio76";
		drive-strength = <8>;	/* 8 mA */
		bias-disable = <0>;   /* no pull */
	};
};

&sde_dsi_suspend {
	mux {
		pins = "gpio75", "gpio76";
		function = "gpio";
	};

	config {
		pins = "gpio75", "gpio76";
		drive-strength = <2>;	/* 2 mA */
		bias-pull-down; 		/* PULL DOWN */
	};
};

