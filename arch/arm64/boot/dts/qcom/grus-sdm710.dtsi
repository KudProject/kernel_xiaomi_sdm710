&tlmm {

	/* GPIO_96 : SPKR_INT */
	smartpa_int_active: smartpa_int_active {
		/* active state */
		mux {
			/* GPIO 96 Interrupt */
			pins = "gpio96";
			function = "gpio";
		};

		config {
			pins = "gpio96";
			drive-strength = <2>; /* 2 MA */
			bias-pull-up;
			input-enable;
		};
	};

	/* GPIO_96 : SPKR_INT */
	smartpa_int_suspend: smartpa_int_suspend {
		/* sleep state */
		mux {
			/* GPIO 96 Interrupt */
			pins = "gpio96";
			function = "gpio";
		};

		config {
			pins = "gpio96";
			drive-strength = <2>; /* 2 MA */
			bias-pull-up;
			input-enable;
		};
	};

	/* GPIO_95 : SPKR_PA_RST */
	smartpa_enable_active: smartpa_enable_active {
		/* active state */
		mux {
			/* GPIO 95 rst pin */
			pins = "gpio95";
			function = "gpio";
		};

		config {
			pins = "gpio95";
			drive-strength = <2>;
			bias-disable;
			bias-pull-down;
			output-low;
		};
	};

	/* GPIO_95 : SPKR_PA_RST */
	smartpa_enable_suspend: smartpa_enable_suspend {
		/* sleep state */
		mux {
			/* GPIO 95 rst pin */
			pins = "gpio95";
			function = "gpio";
		};

		config {
			pins = "gpio95";
			drive-strength = <2>;
			bias-disable;
			bias-pull-down;
			output-low;
		};
	};
};

